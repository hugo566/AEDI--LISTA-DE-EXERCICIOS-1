#include <stdio.h>

int main() {
    float tempo_gasto, velocidade_media, distancia_percorrida, litros_gasolina;
    const float CONSUMO_CARRO = 12.0;  // O carro faz 12 km por litro

    printf("Digite o tempo gasto na viagem (em horas): ");
    scanf("%f", &tempo_gasto);

    printf("Digite a velocidade m√©dia (em km/h): ");
    scanf("%f", &velocidade_media);

    distancia_percorrida = tempo_gasto * velocidade_media;

    litros_gasolina = distancia_percorrida / CONSUMO_CARRO;

    printf("Quantidade de litros de gasolina utilizados: %.2f L\n", litros_gasolina);

    return 0;
}
